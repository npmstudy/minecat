{
  "name": "minecat",
  "version": "0.7.8",
  "description": "a monorepo cli tool for Node.js„ÄÅReact",
  "bin": {
    "minecat": "dist/cli.js"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "author": "npmstudy <npmstudy@qq.com>",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "postinstall.js"
  ],
  "scripts": {
    "build": "tsup src -- --dts-resolve",
    "build:fast": "tsup src",
    "dev": "tsup src --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "postinstall": "node postinstall.js",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "awesome-keywords"
  ],
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/fs-extra": "^11.0.4",
    "@types/minimist": "^1.2.5",
    "@types/prompts": "^2.4.9",
    "@types/shelljs": "^0.8.15",
    "@types/yargs": "^17.0.32",
    "@vitest/coverage-v8": "^1.0.1",
    "@vitest/ui": "^1.3.1",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "happy-dom": "^6.0.4",
    "tmp-promise": "^3.0.3",
    "type-fest": "^4.10.3"
  },
  "dependencies": {
    "@pnpm/workspace.find-packages": "^1.1.11",
    "dclone": "^1.4.2",
    "debug": "^4.3.4",
    "desm": "^1.3.1",
    "find-up": "^7.0.0",
    "libargs": "workspace:*",
    "prompts": "^2.4.2",
    "shelljs": "^0.8.5"
  }
}
